<resources>
    <!-- Location on the screen of the center of the physical power button. -->
    <dimen name="physical_power_button_center_screen_location_y">1010px</dimen>

    <!-- Location on the screen of the center of the physical volume up/down buttons. -->
    <dimen name="physical_volume_up_button_center_screen_location_y">550px</dimen>
    <dimen name="physical_volume_down_button_center_screen_location_y">720px</dimen>

    <!-- Microns/ums (1000 um = 1mm) per pixel for the given device. If unspecified, UI that
         relies on this value will not be sized correctly. -->
    <item name="pixel_pitch" format="float" type="dimen">63.89</item>
</resources>
